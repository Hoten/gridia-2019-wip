{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "object-literal-sort-keys": false
    },
    "rules": {
        "curly": [true, "ignore-same-line"],
        "object-literal-sort-keys": false,
        "quotemark": [true, "single"],
        "interface-name": [true, "never-prefix"],
        "one-variable-per-declaration": false,
        "no-console": false,
        "no-var-requires": false,
        "variable-name": false
    },
    "rulesDirectory": []
}